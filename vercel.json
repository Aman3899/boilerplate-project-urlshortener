{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/shorturl/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/style.css",
            "dest": "/public/style.css"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}